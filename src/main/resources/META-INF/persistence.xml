    <class>sv.edu.occ.ues.ingenieria.prn335.bolsatrabajoprn335.entity.Experiencia</class>
    <class>sv.edu.occ.ues.ingenieria.prn335.bolsatrabajoprn335.entity.Persona</class>
    <class>sv.edu.occ.ues.ingenieria.prn335.bolsatrabajoprn335.entity.TipoDocumento</class>
    <class>sv.edu.occ.ues.ingenieria.prn335.bolsatrabajoprn335.entity.TipoMedioContacto</class>
    <class>sv.edu.occ.ues.ingenieria.prn335.bolsatrabajoprn335.entity.Formacion</class>
    <class>sv.edu.occ.ues.ingenieria.prn335.bolsatrabajoprn335.entity.Documento</class>
    <class>sv.edu.occ.ues.ingenieria.prn335.bolsatrabajoprn335.entity.MedioContacto</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mariadb://localhost:3306/bolsa_trabajo"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="org.mariadb.jdbc.Driver"/>
